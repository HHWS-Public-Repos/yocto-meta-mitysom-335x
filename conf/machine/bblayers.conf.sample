  /home/jenkins/workspace/linux-mitysom-335x-devkit-release/meta-openembedded/meta-networking \
  /home/jenkins/workspace/linux-mitysom-335x-devkit-release/meta \
  /home/jenkins/workspace/linux-mitysom-335x-devkit-release/meta-yocto \
  /home/jenkins/workspace/linux-mitysom-335x-devkit-release/meta-yocto-bsp \
