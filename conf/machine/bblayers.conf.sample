LCONF_VERSION = "6"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  /export/space/yocto_temp/meta \
  /export/space/yocto_temp/meta-yocto \
  /export/space/yocto_temp/meta-yocto-bsp \
  /export/space/yocto_temp/meta-mitysom_335x \
  /export/space/yocto_temp/meta-ti \
  /export/space/yocto_temp/meta-openembedded/meta-oe \
  "
BBLAYERS_NON_REMOVABLE ?= " \
  /export/space/yocto_temp/meta \
  /export/space/yocto_temp/meta-yocto \
  "
